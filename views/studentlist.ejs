<%- include('./base/header.ejs') %>
<div class="cantainer">
    <div class="alert bg-success my-3">
        <h4 class="text-center" style="color: beige;">Student List</h4>
    </div>
    <div class="row">
        <table class = "table text-center bg-success my-3">
            <thead>
                <tr>
                    <th>Roll No</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Father Name</th>
                    <th>Aadhar Card No</th>
                    <th>Mobile No</th>
                </tr>
            </thead>
            <% students.forEach((student)=>{ %>
                <tr>
                    <td><%= student.rollNo %></td>
                    <td><%= student.firstName %></td>
                    <td><%= student.lastName %></td>
                    <td><%= student.fatherName %></td>
                    <td><%= student.adharCardNo %></td>
                    <td><%= student.mobileNo %></td>
                </tr>
            <%})%>
        </table>
    </div>
</div>

<%- include('./base/footer.ejs') %>